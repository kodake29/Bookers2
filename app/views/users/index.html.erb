<div class="row">
	<div class="col-xs-3">
<%= render 'books/list', book_new: @book_new, user: @user %>
</div>
<body>
	<div class="col-xs-9">
	<h2>Users</h2>
<table class="table">
	<thead>
<tr>
<th>image</th>
<th>name</th>
<th></th>
</tr>
</thead>
<tbody>
<% @users.each do |each_user| %>
<tr>
<td>
	<%= attachment_image_tag each_user, :profile_image, :fill, 60, 60, format: 'png', class: "pull-left profile-img", fallback: "noimage.png", size:'60x60' %>
</td>
<td><%= each_user.name %></td>
<td><%= link_to 'Show',user_path(each_user) %></td>
</tr>
<%end%>
</tbody>
</table>
</div>
</body>
</div>