<% @users.each do |user| %>
<table>
<th>image</th>
<th>name</th>
<td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: 'jpeg', class: "img-circle pull-left profile-img", fallback: "noimage.png" %></td>
<td><%= user.name %></td>
<td><%= link_to 'Show',user_path(user) %></td>
<%end%>
