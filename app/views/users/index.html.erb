<body>
<table>
<tr>
<th>image</th>
<th>name</th>
</tr>
<% @users.each do |user| %>
<tr>
<td><%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: 'png', class: "img-circle pull-left profile-img", fallback: "noimage.png", size:'60x60' %></td>
<td><%= user.name %></td>
<td><%= link_to 'Show',user_path(user) %></td>
</tr>
<%end%>
</table>
</body>