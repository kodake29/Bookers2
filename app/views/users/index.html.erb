<%= render 'books/list', book_new: @book_new, user: @user %>
<body>
	<h2>Users</h2>
<table>
<tr>
<th>image</th>
<th>name</th>
</tr>
<% @users.each do |each_user| %>
<tr>
<td>
	<%= attachment_image_tag each_user, :profile_image, :fill, 60, 60, format: 'png', class: "img-circle pull-left profile-img", fallback: "noimage.png", size:'60x60' %>
</td>
<td><%= each_user.name %></td>
<td><%= link_to 'Show',user_path(each_user) %></td>
</tr>
<%end%>
</table>
</body>